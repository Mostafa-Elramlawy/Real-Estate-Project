                                        <div class="detail-wrapper-body">
                                            <div class="listing-title-bar">
                                                <h3>
                                                    @Html.DisplayFor(model => model.Property.Title)<span class="mrg-l-5 category-tag">@Html.DisplayFor(model => model.Status_ID)</span>
                                                </h3>
                                                <div class="mt-0">
                                                    <a class="listing-address">
                                                        @Html.DisplayFor(model => model.Property.City_id)
                                                    </a>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="single detail-wrapper mr-2" style="padding-left: 270px;">
                                            <div class="detail-wrapper-body">
                                                <div class="listing-title-bar">
                                                    <h4 style="padding: 15px;">@Html.DisplayFor(model => model.Property.Price)EG</h4>

                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </section>
                                <!-- main slider carousel items -->
                                <div id="listingDetailsSlider" class="carousel listing-details-sliders slide mb-30">
                                    <h5 class="mb-4">Gallery</h5>
                                    <div id="listingDetailsSlider" class="carousel slide" data-ride="carousel">
                                        <div class="carousel-inner">
                                            @foreach (var image in Model.PropertyImages)
                                            {
                                                <div class="item @(image == Model.PropertyImages.First() ? "active" : "")">
                                                    <img src="@Url.Content(image.img_path)" class="img-fluid" alt="slider-listing">
                                                </div>
                                            }
                                        </div>
                                        <a class="carousel-control left" href="#listingDetailsSlider" data-slide="prev"><i class="fa fa-angle-left" style="margin: 0px;margin-top: 6px;margin-right: 2px;"></i></a>
                                        <a class="carousel-control right" href="#listingDetailsSlider" data-slide="next" style="padding-right: 10px;"><i class="fa fa-angle-right"></i></a>
                                    </div>
                                    <ul class="carousel-indicators smail-listing list-inline">
                                        @foreach (var image in Model.PropertyImages)
                                        {
                                            <li class="list-inline-item @(image == Model.PropertyImages.First() ? "active" : "")">
                                                <a id="carousel-selector-@(image.Pro_id)" data-slide-to="@(image.Pro_id)" data-target="#listingDetailsSlider">
                                                    <img src="@Url.Content(image.img_path)" class="img-fluid" alt="listing-small">
                                                </a>
                                            </li>
                                        }
                                    </ul>
                                </div>
                                <div class="blog-info details mb-30">
                                    <h5 class="mb-4">Description</h5>
                                    <p class="mb-3">
                                        @Html.DisplayFor(model => model.Property.Description)
                                    </p>
                                </div>
                            </div>
