        <script>
        $(document).ready(function () {
        // Search button click event
        $('#search-button').click(function (e) {
            e.preventDefault(); // Prevent the default form submission

            var keyword = $('#keyword-input').val(); // Get the entered keyword

            // Perform the search (example: filtering the properties)
            var results = filterProperties(keyword);

            // Display the search results
            displayResults(results);
        });

        // Function to filter properties based on the keyword
        function filterProperties(keyword) {
            var results = []; // Array to store the filtered properties

            // Perform the filtering logic here based on the keyword and your data

            // Example filtering logic: check if the property title contains the keyword
            @foreach (var property in Model)
            {
                var title = property.Property.Title;
                if (title.ToLower().Contains(keyword.ToLower()))
                {
                    results.Add(property.Property.Title);
                }
            }

            return results;
        }

        // Function to display the search results
            function displayResults(results) {
                var container = $('#search-results');

                // Clear previous results
                container.empty();

                // Check if any results found
                if (results.length > 0) {
                    // Iterate over the results and append them to the container
                    $.each(results, function (index, result) {
                        container.append('<p>' + result + '</p>');
                    });
                } else {
                    // No results found
                    container.append('<p>No results found.</p>');
                }
            }
        });
        </script>
